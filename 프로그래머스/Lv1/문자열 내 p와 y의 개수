#python
def solution(s):
    s = s.upper()

    if s.count("P") != s.count("Y"):
        return False

    return True

#js
function solution(s){
   
    s = s.toUpperCase()
    
    if(s.split("P").length != s.split("Y").length)
        return false;
    else
        return true;   
}

#js
function solution(s){
    
    if( s.match(/p/ig).length !== s.match(/y/ig).length )
        return false;
    else
        return true;
}